{
	"user": "mobutils",
	"subscribe": [
		"gsm.modem*"
	],
	"listen": [ 
		"ubus.object.remove",
		"ubus.object.add",
		"sim.changed"
	],
	"publish": [
		"urcd.modem*"
	],
	"access": {
		"gsm": {
			"methods": [
				"info"
			]
		},
		"gsm.modem*": {
			"methods": [
				"info",
				"read_sms",
				"delete_sms",
				"get_sim_slot",
				"get_default_modem_func",
				"get_incoming_voice_calls",
				"accept_voice_call",
				"reject_voice_call"
			]
		},
		"ip_block": {
			"methods": [
				"show"
			]
		},
		"log": {
			"methods": [ "write_ext" ]
		}
	}
}
