#!/bin/sh /etc/rc.common

START=14
STOP=50
USE_PROCD=1

start_service() {
	procd_open_instance
	procd_set_param command "/usr/sbin/urcd"
	procd_set_param respawn
	procd_set_param stderr 1
	procd_set_param file "/etc/config/call_utils"
	procd_close_instance
}

service_triggers()
{
	procd_add_reload_trigger "call_utils"
}
