{
    "user": "mqtt_pub",
    "access": {
        "ioman.*": {
            "methods": [
                "status",
                "location"
            ]
        },
        "gsm": {
            "methods": [
                "info"
            ]
        },
        "gsm.modem*": {
            "methods": [
                "info",
                "get_network_info",
                "get_net_reg_stat_all",
                "get_signal_query",
                "get_service_provider",
                "get_temperature"
            ]
        },
        "network.interface": {
            "methods": [
                "dump"
            ]
        }
    }
}
