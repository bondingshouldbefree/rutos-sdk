config defaults '1'
	option syn_flood '1'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'

config zone '2'
	option name 'lan'
	list network 'lan'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'

config redirect '3'
	list proto 'udp'
	option name 'Redirect_hostname'
	option target 'DNAT'
	option src 'lan'
	option src_dport '53'
	option dest_ip '127.0.0.1'
	option extra '-m string --hex-string |06|tap100|05|rutos|03|net| --algo bm'
	option priority '1'
