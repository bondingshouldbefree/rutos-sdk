#include <dt-bindings/input/input.h>
#include "mt7621.dtsi"
#include "platform_name.dtsi"

&pcie {
    status = "okay";
};

&gmac0 {
	nvmem-cells = <&macaddr_config_0 1>;
	nvmem-cell-names = "mac-address";
};

&gmac1 {
        status = "okay";
	label = "lan1";
	phy-handle = <&ethphy4>;
	nvmem-cells = <&macaddr_config_0 0>;
	nvmem-cell-names = "mac-address";
};

&ethphy4 {
	/delete-property/ interrupts;
};

&spi0 {
    status = "okay";
    cs-gpios = <&gpio 34 GPIO_ACTIVE_HIGH>, <&gpio 35 GPIO_ACTIVE_HIGH>;

    flash@0 {
		compatible = "w25q128","jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			boot: partition@0 {
				label = "u-boot";
				reg = <0x0 0x80000>;
				read-only;
			};

			partition@80000 {
				label = "u-boot-env";
				reg = <0x80000 0x10000>;
				read-only;
			};

			config: partition@90000 {
				label = "config";
				reg = <0x90000 0x10000>;
				read-only;

				nvmem-layout {
					compatible = "fixed-layout";
					#address-cells = <1>;
					#size-cells = <1>;

					macaddr_config_0: mac-address@0 {
						compatible = "mac-base";
						reg = <0x0 0x6>;
						#nvmem-cell-cells = <1>;
					};
				};
			};

			partition@a0000 {
				label = "bootconfig-a";
				reg = <0xa0000 0x10000>;
				read-only;
			};

			partition@b0000 {
				label = "bootconfig-b";
				reg = <0xb0000 0x10000>;
				read-only;
			};

			partition@d0000 {
				label = "event-log";
				reg = <0xc0000 0x90000>;
			};

			partition@400000 {
				label = "recovery";
				reg = <0x200000 0xe00000>;
			};
		};
	};

    flash@1 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spi-nand","spinand,mt29f";
		reg = <1>;
		spi-max-frequency = <24000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "rutos-a";
				reg = <0x00000000 0x08000000>;
			};

			partition@8000000 {
				label = "rutos-b";
				reg = <0x08000000 0x08000000>;
			};
		};
	};
};
